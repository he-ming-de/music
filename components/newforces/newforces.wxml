<!--components/newforces/newforces.wxml-->

<view class="item1">
	<view class="fonts">
		<view class="bos">新势力</view>
		<view class="fontss">新音乐</view>
	</view>
	<view class="item">
		<view wx:for="{{results}}" wx:key="index" class="item3" bindtap="bindplay" data-item="{{item}}">
			<view class="imgs">
				<image src="{{item.picUrl}}" class="img"></image>
			</view>
			<view class="font">{{item.name}}</view>
		</view>
	</view>
</view>